[agent]
instanceid=7
groupid=2
clusterid=1
tmpdir=/tmp/agent
[log]
config=/etc/IMAgent/logging.cnf
section=normal

[database]
name=Mongodb
attr_home=/usr/
attr_config=/etc/mongodb.conf
attr_user=
attr_password=
attr_backupdir=/db/mongodb/backup

[monitors]
monitors=StatusMonitor
interval_Active=300
interval_Sender=30
interval_SystemMonitor=20
interval_HearbeatMonitor=20
interval_StatusMonitor=20
replace=os.disk.dev252-16|sd_sys;os.disk.dev252-16|sd_data;os.disk.dev252-16|sd_log;os.disk.dev253-0|sdp_data
ignore=os.disk_sdd

[rabbitmq]
managerip=rabbitmq
rmqport=5672


